{
	"name": "Test-Vitor-Monteiro",
	"version": "1.0.0",
	"description": "Teste mailou dev",
	"main": "index.js",
	"scripts": {
		"build": "npx tsc --build",
		"start": "export NODE_ENV='production' && node lib/index.js",
		"start_windows": "SET NODE_ENV=production&& node lib/index.js",
		"dev": "SET NODE_ENV=staging&& tsx watch index.ts",
		"dev_unix": "export NODE_ENV='staging' && tsx src/index.ts",
		"migrate": "node-pg-migrate -m ./db/migrations --tsx true",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
	},
	"license": "MIT",
	"private": true,
	"dependencies": {
		"dotenv": "^16.4.5",
		"fastify": "^5.1.0",
		"pg": "^8.13.1",
		"pg-hstore": "^2.3.4",
		"tsx": "^4.19.2",
		"typescript": "^5.6.3"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.14.0",
		"@types/node": "^22.8.6",
		"@types/pg": "^8.11.10",
		"@types/uuid": "^10.0.0",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.11.0",
		"node-pg-migrate": "^7.7.1",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"typescript-eslint": "^8.12.2"
	}
}
